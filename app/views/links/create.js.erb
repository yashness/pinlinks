	$('#' + 'repo_list').append("<tr id= <%='repo_record_' + @repo.id.to_s%> ><%= escape_javascript(render(:partial => 'shared/repo_record', :locals => {:repo => @repo ,:links => @repo.links , :user => current_user, :owner => true} )) %></tr>");
}
